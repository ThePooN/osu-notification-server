{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "indent": [true, "spaces", 2],
      "interface-name": false,
      "max-line-length": [
        true, {
          "ignore-pattern": "// https://",
          "limit": 120
        }
      ],
      "member-access": [true, "no-public"],
      "member-ordering": [
        true,
        {
          "order": "statics-first",
          "alphabetize": true
        }
      ],
      "no-console": false,
      "quotemark": [true, "single"]
    },
    "rulesDirectory": []
}
